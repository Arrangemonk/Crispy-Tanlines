; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="CrispyTanlines"
run/main_scene="res://Scenes/Intro.tscn"

[autoload]

Global="*res://Code/Global.cs"
DragDropContainer="*res://Code/DragDropContainer.cs"

[debug]

settings/fps/force_fps=60

[display]

window/size/width=1280
window/size/height=720

[gui]

common/drop_mouse_on_gui_input_disabled=true

[mono]

project/assembly_name="CrispTanlines"

[physics]

3d/smooth_trimesh_collision=true
common/enable_pause_aware_picking=true
3d/physics_engine="Bullet"

[rendering]

vram_compression/import_etc=true
vram_compression/import_etc2=false
quality/directional_shadow/size.mobile=4096
quality/shadow_atlas/cubemap_size=1024
quality/shadows/filter_mode.mobile=1
quality/shading/force_vertex_shading.mobile=false
quality/lightmapping/use_bicubic_sampling.mobile=true
quality/filters/msaa=2
quality/filters/use_fxaa=true
quality/filters/use_debanding=true
